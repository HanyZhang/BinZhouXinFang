<!doctype html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>网站后台管理系统</title>
    <link rel="shortcut icon" href="~/Content/images/favicon.ico" />
    <link href="~/Content/css/css.css" type="text/css" rel="stylesheet" />
</head>
<!--框架样式-->
<frameset rows="95,*,30" cols="*" frameborder="no" border="0" framespacing="0">
    <!--top样式-->
    <frame src="../_Header" name="topframe" scrolling="no" noresize id="topframe" title="topframe">@Html.Partial("_Header")</frame>
    <!--contact样式-->
    <frameset id="attachucp" framespacing="0" border="0" frameborder="no" cols="194,12,*" rows="*">
        <frame scrolling="auto" noresize="" frameborder="no" name="leftFrame" src="../_Menu">@Html.Partial("_Menu")</frame>
        <frame id="leftbar" scrolling="no" noresize="" name="switchFrame" src="../_Switch">@Html.Partial("_Switch")</frame>
        <frame scrolling="auto" noresize="" border="0" name="mainFrame" src="">@RenderBody()</frame>
    </frameset>
    <!--bottom样式-->
    <frame src="../_Bottom" name="bottomFrame" scrolling="No" noresize="noresize" id="bottomFrame" title="bottomFrame">@Html.Partial("_Bottom")</frame>
</frameset>
<noframes></noframes>
<!--不可以删除-->
</html>
