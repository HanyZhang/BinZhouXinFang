@model IEnumerable<PetitionLetter.Dll.Model.User>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="easyui-panel" style="width:100%;height:100%;padding:10px" title=@ViewBag.Title>
    <div style="display:flex;flex-wrap:wrap">
        @foreach(var item in Model)
        {
            
            var name = item.CountyName + "信访办";
            <a style="margin-right:30px">@name</a>
        }
    </div>
</div>


